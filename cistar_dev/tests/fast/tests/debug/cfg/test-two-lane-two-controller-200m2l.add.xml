<?xml version='1.0' encoding='UTF-8'?>
<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">
  <route edges="left bottom right top" id="routeleft"/>
  <route edges="bottom right top left" id="routebottom"/>
  <route edges="top left bottom right" id="routetop"/>
  <route edges="right top left bottom" id="routeright"/>
  <rerouter edges="top" id="rerouterTop">
    <interval begin="0" end="10000000">
      <routeProbReroute id="routebottom"/>
    </interval>
  </rerouter>
  <rerouter edges="bottom" id="rerouterBottom">
    <interval begin="0" end="10000000">
      <routeProbReroute id="routetop"/>
    </interval>
  </rerouter>
  <rerouter edges="left" id="rerouterLeft">
    <interval begin="0" end="10000000">
      <routeProbReroute id="routeright"/>
    </interval>
  </rerouter>
  <rerouter edges="right" id="rerouterRight">
    <interval begin="0" end="10000000">
      <routeProbReroute id="routeleft"/>
    </interval>
  </rerouter>
</additional>
